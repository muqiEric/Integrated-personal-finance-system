tent":["<template>\r\n  <div class=\"expense-stats-page\">\r\n    <h1>支出统计</h1>\r\n\r\n    <!-- 数据表格 -->\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>交易ID</th>\r\n        <th>交易时间</th>\r\n        <th>交易对象</th>\r\n        <th>商品名称</th>\r\n        <th>金额</th>\r\n        <th>支付方式</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in expenseTransactions\" :key=\"index\">\r\n        <td>{{ item.id }}</td>\r\n        <td>{{ item.trans_time }}</td>\r\n        <td>{{ item.trans_party }}</td>\r\n        <td>{{ item.goods_name }}</td>\r\n        <td>{{ item.amount }}</td>\r\n        <td>{{ item.payment_method }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- 饼状图 -->\r\n    <canvas id=\"expenseChart\"></canvas>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted, ref } from \"vue\";\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  name: \"ExpenseStatsPage\",\r\n  props: {\r\n    transactions: Array,\r\n  },\r\n  setup(props) {\r\n    const expenseTransactions = ref(\r\n        props.transactions.filter((item) => item.trans_type === \"支出\")\r\n    );\r\n\r\n    onMounted(() => {\r\n      const ctx = document.getElementById(\"expenseChart\").getContext(\"2d\");\r\n      const expenseData = expenseTransactions.value.reduce((acc, curr) => {\r\n        const key = curr.goods_name || \"其他\";\r\n        acc[key] = (acc[key] || 0) + Math.abs(curr.amount);\r\n        return acc;\r\n      }, {});\r\n\r\n      new Chart(ctx, {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: Object.keys(expenseData),\r\n          datasets: [\r\n            {\r\n              label: \"支出统计\",\r\n              data: Object.values(expenseData),\r\n              backgroundColor: [\"#FF6384\", \"#36A2EB\", \"#FFCE56\", \"#4BC0C0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    });\r\n\r\n    return { expenseTransactions };\r\n  },\r\n};\r\n</script>\r\n\r\n<sty