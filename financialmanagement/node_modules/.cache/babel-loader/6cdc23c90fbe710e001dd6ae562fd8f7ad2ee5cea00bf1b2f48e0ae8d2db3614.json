nagement\\src\\views\\IncomeStatsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"income-stats-page\">\r\n    <h1>收入统计</h1>\r\n\r\n    <!-- 数据表格 -->\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>交易ID</th>\r\n        <th>交易时间</th>\r\n        <th>交易对象</th>\r\n        <th>商品名称</th>\r\n        <th>金额</th>\r\n        <th>支付方式</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in incomeTransactions\" :key=\"index\">\r\n        <td>{{ item.id }}</td>\r\n        <td>{{ item.trans_time }}</td>\r\n        <td>{{ item.trans_party }}</td>\r\n        <td>{{ item.goods_name }}</td>\r\n        <td>{{ item.amount }}</td>\r\n        <td>{{ item.payment_method }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- 饼状图 -->\r\n    <div>\r\n      <canvas id=\"incomeChart\"></canvas>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted, ref } from \"vue\";\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  name: \"IncomeStatsPage\",\r\n  props: {\r\n    transactions: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n  },\r\n  setup(props) {\r\n    const incomeTransactions = ref(\r\n        props.transactions.filter((item) => item.trans_type === \"收入\")\r\n    );\r\n\r\n    // 饼图绘制\r\n    onMounted(() => {\r\n      const ctx = document.getElementById(\"incomeChart\").getContext(\"2d\");\r\n      const incomeData = incomeTransactions.value.reduce((acc, curr) => {\r\n        const type = curr.goods_name;\r\n        if (!acc[type]) acc[type] = 0;\r\n        acc[type] += curr.amount;\r\n        return acc;\r\n      }, {});\r\n\r\n      new Chart(ctx, {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: Object.keys(incomeData),\r\n          datasets: [\r\n            {\r\n              data: Object.values(incomeData),\r\n              backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\", \"#4BC0C0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    });\r\n\r\n    return { incomeTransactions };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin: 20px 0;\r\n}\r\n\r\nthead th {\r\n  background-color: #f2f2f2;\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\ntbody td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\ncanvas {\r\n  margin-top: 20px;\r\n  width: 400px;\r\n  height: 400px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AAoCA,SAASA,SAAS,EAAEC,GAAE,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEC,KAAK;MACX,WAAS,SAATC,QAAOA,CAAA;QAAA,OAAQ,EAAE;MAAA;IACnB;EACF,CAAC;EACDC,KAAK,WAALA,KAAKA,CAACL,KAAK,EAAE;IACX,IAAMM,kBAAiB,GAAIT,GAAG,CAC1BG,KAAK,CAACC,YAAY,CAACM,MAAM,CAAC,UAACC,IAAI;MAAA,OAAKA,IAAI,CAACC,UAAS,KAAM,IAAI;IAAA,EAChE,CAAC;;IAED;IACAb,SAAS,CAAC,YAAM;MACd,IAAMc,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MACnE,IAAMC,UAAS,GAAIR,kBAAkB,CAACS,KAAK,CAACC,MAAM,CAAC,UAACC,GAAG,EAAEC,IAAI,EAAK;QAChE,IAAMhB,IAAG,GAAIgB,IAAI,CAACC,UAAU;QAC5B,IAAI,CAACF,GAAG,CAACf,IAAI,CAAC,EAAEe,GAAG,CAACf,IAAI,IAAI,CAAC;QAC7Be,GAAG,CAACf,IAAI,KAAKgB,IAAI,CAACE,MAAM;QACxB,OAAOH,GAAG;MACZ,CAAC,EAAE,CAAC,CAAC,CAAC;MAEN,IAAInB,KAAK,CAACY,GAAG,EAAE;QACbR,IAAI,EAAE,KAAK;QACXmB,IAAI,EAAE;UACJC,MAAM,EAAEC,MAAM,CAACC,IAAI,CAACV,UAAU,CAAC;UAC/BW,QAAQ,EAAE,CACR;YACEJ,IAAI,EAAEE,MAAM,CAACG,MAAM,CAACZ,UAAU,CAAC;YAC/Ba,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS;UAC9D,CAAC;QAEL;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;IAEF,OAAO;MAAErB,kBAAiB,EAAjBA;IAAmB,CAAC;EAC/B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}