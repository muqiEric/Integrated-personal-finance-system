{"ast":null,"code":"import \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.reduce.js\";\nimport \"core-js/modules/es.object.keys.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.object.values.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport { onMounted, ref } from \"vue\";\nimport Chart from \"chart.js/auto\";\nexport default {\n  props: {\n    transactions: {\n      type: Array,\n      required: true\n    }\n  },\n  setup: function setup(props) {\n    // 筛选收入数据\n    var incomeTransactions = ref(props.transactions.filter(function (item) {\n      return item.trans_type === \"收入\";\n    }));\n\n    // 绘制饼状图\n    onMounted(function () {\n      if (incomeTransactions.value.length === 0) return; // 如果没有收入数据，退出\n\n      var ctx = document.getElementById(\"incomeChart\").getContext(\"2d\");\n      var incomeData = incomeTransactions.value.reduce(function (acc, curr) {\n        var key = curr.goods_name || \"其他\";\n        acc[key] = (acc[key] || 0) + curr.amount;\n        return acc;\n      }, {});\n      new Chart(ctx, {\n        type: \"pie\",\n        data: {\n          labels: Object.keys(incomeData),\n          datasets: [{\n            data: Object.values(incomeData),\n            backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\", \"#4BC0C0\"]\n          }]\n        }\n      });\n    });\n    return {\n      incomeTransactions: incomeTransactions\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","Chart","props","transactions","type","Array","required","setup","incomeTransactions","filter","item","trans_type","value","length","ctx","document","getElementById","getContext","incomeData","reduce","acc","curr","key","goods_name","amount","data","labels","Object","keys","datasets","values","backgroundColor"],"sources":["C:\\Users\\20915\\IdeaProjects\\financialmanagement\\src\\views\\IncomeStatsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"income-stats-page\">\r\n    <h1>收入统计</h1>\r\n\r\n    <!-- 数据表格 -->\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>交易ID</th>\r\n        <th>交易时间</th>\r\n        <th>交易对象</th>\r\n        <th>商品名称</th>\r\n        <th>金额</th>\r\n        <th>支付方式</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in incomeTransactions\" :key=\"index\">\r\n        <td>{{ item.id }}</td>\r\n        <td>{{ item.trans_time }}</td>\r\n        <td>{{ item.trans_party }}</td>\r\n        <td>{{ item.goods_name }}</td>\r\n        <td>{{ item.amount }}</td>\r\n        <td>{{ item.payment_method }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- 饼状图 -->\r\n    <canvas id=\"incomeChart\"></canvas>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted, ref } from \"vue\";\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  props: {\r\n    transactions: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  setup(props) {\r\n    // 筛选收入数据\r\n    const incomeTransactions = ref(\r\n        props.transactions.filter((item) => item.trans_type === \"收入\")\r\n    );\r\n\r\n    // 绘制饼状图\r\n    onMounted(() => {\r\n      if (incomeTransactions.value.length === 0) return; // 如果没有收入数据，退出\r\n\r\n      const ctx = document.getElementById(\"incomeChart\").getContext(\"2d\");\r\n      const incomeData = incomeTransactions.value.reduce((acc, curr) => {\r\n        const key = curr.goods_name || \"其他\";\r\n        acc[key] = (acc[key] || 0) + curr.amount;\r\n        return acc;\r\n      }, {});\r\n\r\n      new Chart(ctx, {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: Object.keys(incomeData),\r\n          datasets: [\r\n            {\r\n              data: Object.values(incomeData),\r\n              backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\", \"#4BC0C0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    });\r\n\r\n    return { incomeTransactions };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 页面样式，与主页面和支出页面保持一致 */\r\n.income-stats-page {\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n  color: #333;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nthead th {\r\n  background-color: #f2f2f2;\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\ntbody td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\ncanvas {\r\n  margin-top: 20px;\r\n  width: 400px;\r\n  height: 400px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AAkCA,SAASA,SAAS,EAAEC,GAAE,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,WAALA,KAAKA,CAACL,KAAK,EAAE;IACX;IACA,IAAMM,kBAAiB,GAAIR,GAAG,CAC1BE,KAAK,CAACC,YAAY,CAACM,MAAM,CAAC,UAACC,IAAI;MAAA,OAAKA,IAAI,CAACC,UAAS,KAAM,IAAI;IAAA,EAChE,CAAC;;IAED;IACAZ,SAAS,CAAC,YAAM;MACd,IAAIS,kBAAkB,CAACI,KAAK,CAACC,MAAK,KAAM,CAAC,EAAE,OAAM,CAAE;;MAEnD,IAAMC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MACnE,IAAMC,UAAS,GAAIV,kBAAkB,CAACI,KAAK,CAACO,MAAM,CAAC,UAACC,GAAG,EAAEC,IAAI,EAAK;QAChE,IAAMC,GAAE,GAAID,IAAI,CAACE,UAAS,IAAK,IAAI;QACnCH,GAAG,CAACE,GAAG,IAAI,CAACF,GAAG,CAACE,GAAG,KAAK,CAAC,IAAID,IAAI,CAACG,MAAM;QACxC,OAAOJ,GAAG;MACZ,CAAC,EAAE,CAAC,CAAC,CAAC;MAEN,IAAInB,KAAK,CAACa,GAAG,EAAE;QACbV,IAAI,EAAE,KAAK;QACXqB,IAAI,EAAE;UACJC,MAAM,EAAEC,MAAM,CAACC,IAAI,CAACV,UAAU,CAAC;UAC/BW,QAAQ,EAAE,CACR;YACEJ,IAAI,EAAEE,MAAM,CAACG,MAAM,CAACZ,UAAU,CAAC;YAC/Ba,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS;UAC9D,CAAC;QAEL;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;IAEF,OAAO;MAAEvB,kBAAiB,EAAjBA;IAAmB,CAAC;EAC/B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}