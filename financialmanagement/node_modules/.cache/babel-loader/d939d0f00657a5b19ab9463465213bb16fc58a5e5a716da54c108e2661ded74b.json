{"ast":null,"code":"import \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.reduce.js\";\nimport \"core-js/modules/es.object.keys.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.object.values.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport { onMounted, ref } from \"vue\";\nimport Chart from \"chart.js/auto\";\nexport default {\n  props: {\n    transactions: {\n      type: Array,\n      required: true\n    }\n  },\n  setup: function setup(props) {\n    // 筛选支出数据\n    var expenseTransactions = ref(props.transactions.filter(function (item) {\n      return item.trans_type === \"支出\";\n    }));\n\n    // 绘制饼状图\n    onMounted(function () {\n      if (expenseTransactions.value.length === 0) return; // 如果没有支出数据，退出\n\n      var ctx = document.getElementById(\"expenseChart\").getContext(\"2d\");\n      var expenseData = expenseTransactions.value.reduce(function (acc, curr) {\n        var key = curr.goods_name || \"其他\";\n        acc[key] = (acc[key] || 0) + Math.abs(curr.amount); // 金额取绝对值\n        return acc;\n      }, {});\n      new Chart(ctx, {\n        type: \"pie\",\n        data: {\n          labels: Object.keys(expenseData),\n          datasets: [{\n            data: Object.values(expenseData),\n            backgroundColor: [\"#FF6384\", \"#36A2EB\", \"#FFCE56\", \"#4BC0C0\"]\n          }]\n        }\n      });\n    });\n    return {\n      expenseTransactions: expenseTransactions\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","Chart","props","transactions","type","Array","required","setup","expenseTransactions","filter","item","trans_type","value","length","ctx","document","getElementById","getContext","expenseData","reduce","acc","curr","key","goods_name","Math","abs","amount","data","labels","Object","keys","datasets","values","backgroundColor"],"sources":["C:\\Users\\20915\\IdeaProjects\\financialmanagement\\src\\views\\ExpenseStatsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"expense-stats-page\">\r\n    <h1>支出统计</h1>\r\n\r\n    <!-- 数据表格 -->\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>交易ID</th>\r\n        <th>交易时间</th>\r\n        <th>交易对象</th>\r\n        <th>商品名称</th>\r\n        <th>金额</th>\r\n        <th>支付方式</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"(item, index) in expenseTransactions\" :key=\"index\">\r\n        <td>{{ item.id }}</td>\r\n        <td>{{ item.trans_time }}</td>\r\n        <td>{{ item.trans_party }}</td>\r\n        <td>{{ item.goods_name }}</td>\r\n        <td>{{ item.amount }}</td>\r\n        <td>{{ item.payment_method }}</td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- 饼状图 -->\r\n    <canvas id=\"expenseChart\"></canvas>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { onMounted, ref } from \"vue\";\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  props: {\r\n    transactions: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  setup(props) {\r\n    // 筛选支出数据\r\n    const expenseTransactions = ref(\r\n        props.transactions.filter((item) => item.trans_type === \"支出\")\r\n    );\r\n\r\n    // 绘制饼状图\r\n    onMounted(() => {\r\n      if (expenseTransactions.value.length === 0) return; // 如果没有支出数据，退出\r\n\r\n      const ctx = document.getElementById(\"expenseChart\").getContext(\"2d\");\r\n      const expenseData = expenseTransactions.value.reduce((acc, curr) => {\r\n        const key = curr.goods_name || \"其他\";\r\n        acc[key] = (acc[key] || 0) + Math.abs(curr.amount); // 金额取绝对值\r\n        return acc;\r\n      }, {});\r\n\r\n      new Chart(ctx, {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: Object.keys(expenseData),\r\n          datasets: [\r\n            {\r\n              data: Object.values(expenseData),\r\n              backgroundColor: [\"#FF6384\", \"#36A2EB\", \"#FFCE56\", \"#4BC0C0\"],\r\n            },\r\n          ],\r\n        },\r\n      });\r\n    });\r\n\r\n    return { expenseTransactions };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 页面样式，与主页面和收入页面保持一致 */\r\n.expense-stats-page {\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n  color: #333;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nthead th {\r\n  background-color: #f2f2f2;\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\ntbody td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\ncanvas {\r\n  margin-top: 20px;\r\n  width: 400px;\r\n  height: 400px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AAkCA,SAASA,SAAS,EAAEC,GAAE,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,KAAK,EAAE;IACLC,YAAY,EAAE;MACZC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,WAALA,KAAKA,CAACL,KAAK,EAAE;IACX;IACA,IAAMM,mBAAkB,GAAIR,GAAG,CAC3BE,KAAK,CAACC,YAAY,CAACM,MAAM,CAAC,UAACC,IAAI;MAAA,OAAKA,IAAI,CAACC,UAAS,KAAM,IAAI;IAAA,EAChE,CAAC;;IAED;IACAZ,SAAS,CAAC,YAAM;MACd,IAAIS,mBAAmB,CAACI,KAAK,CAACC,MAAK,KAAM,CAAC,EAAE,OAAM,CAAE;;MAEpD,IAAMC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MACpE,IAAMC,WAAU,GAAIV,mBAAmB,CAACI,KAAK,CAACO,MAAM,CAAC,UAACC,GAAG,EAAEC,IAAI,EAAK;QAClE,IAAMC,GAAE,GAAID,IAAI,CAACE,UAAS,IAAK,IAAI;QACnCH,GAAG,CAACE,GAAG,IAAI,CAACF,GAAG,CAACE,GAAG,KAAK,CAAC,IAAIE,IAAI,CAACC,GAAG,CAACJ,IAAI,CAACK,MAAM,CAAC,EAAE;QACpD,OAAON,GAAG;MACZ,CAAC,EAAE,CAAC,CAAC,CAAC;MAEN,IAAInB,KAAK,CAACa,GAAG,EAAE;QACbV,IAAI,EAAE,KAAK;QACXuB,IAAI,EAAE;UACJC,MAAM,EAAEC,MAAM,CAACC,IAAI,CAACZ,WAAW,CAAC;UAChCa,QAAQ,EAAE,CACR;YACEJ,IAAI,EAAEE,MAAM,CAACG,MAAM,CAACd,WAAW,CAAC;YAChCe,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS;UAC9D,CAAC;QAEL;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;IAEF,OAAO;MAAEzB,mBAAkB,EAAlBA;IAAoB,CAAC;EAChC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies