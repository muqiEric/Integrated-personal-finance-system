Bar from \"@/components/NavigationBar.vue\";\r\n\r\nexport default {\r\n  name: \"MyPage\",\r\n  components: {\r\n    NavigationBar,\r\n  },\r\n  data() {\r\n    return {\r\n      userId: \"12345\", // 示例用户ID\r\n      newFamilyMember: { account: \"\", password: \"\" }, // 输入框绑定的对象\r\n      familyMembers: [], // 存储成功添加的家庭成员（只显示姓名）\r\n      // 模拟家庭成员数据库\r\n      familyMemberDatabase: [\r\n        { name: \"张三\", account: \"zhangsan\", password: \"123456\" },\r\n        { name: \"李四\", account: \"lisi\", password: \"password\" },\r\n        { name: \"王五\", account: \"wangwu\", password: \"qwerty\" },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    // 添加家庭成员\r\n    addFamilyMember() {\r\n      const { account, password } = this.newFamilyMember;\r\n\r\n      if (!account || !password) {\r\n        alert(\"请填写账号和密码！\");\r\n        return;\r\n      }\r\n\r\n      // 验证账号和密码是否匹配\r\n      const member = this.familyMemberDatabase.find(\r\n          (m) => m.account === account && m.password === password\r\n      );\r\n\r\n      if (!member) {\r\n        alert(\"账号或密码错误！\");\r\n        return;\r\n      }\r\n\r\n      // 检查是否已存在\r\n      if (this.familyMembers.some((m) => m.name === member.name)) {\r\n        alert(\"该家庭成员已添加！\");\r\n        return;\r\n      }\r\n\r\n      // 添加成员\r\n      this.familyMembers.push({ name: member.name });\r\n      this.newFamilyMember = { account: \"\", password: \"\" }; // 清空输入框\r\n    },\r\n\r\n    // 删除家庭成员\r\n    removeFamilyMember(index) {\r\n      this.familyMembers.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-page-content {\r\n  margin: 20px;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.user-info {\r\n  margin-top: 20px;\r\n}\r\n\r\n.family-members {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.family-members ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.family-members li {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  margin-bottom: 5px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.remove-btn {\r\n  background-color: #dc3545;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-btn:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n.add-member {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.add-member input {\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  width: 250px;\r\n}\r\n\r\n.add-btn {\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  padding: 8px 15px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.add-btn:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;;AA6CA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVF,aAAa,EAAbA;EACF,CAAC;EACDG,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,OAAO;MAAE;MACjBC,eAAe,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAG,CAAC;MAAE;MAChDC,aAAa,EAAE,EAAE;MAAE;MACnB;MACAC,oBAAoB,EAAE,CACpB;QAAER,IAAI,EAAE,IAAI;QAAEK,OAAO,EAAE,UAAU;QAAEC,QAAQ,EAAE;MAAS,CAAC,EACvD;QAAEN,IAAI,EAAE,IAAI;QAAEK,OAAO,EAAE,MAAM;QAAEC,QAAQ,EAAE;MAAW,CAAC,EACrD;QAAEN,IAAI,EAAE,IAAI;QAAEK,OAAO,EAAE,QAAQ;QAAEC,QAAQ,EAAE;MAAS,CAAC;IAEzD,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACP;IACAC,eAAe,WAAfA,eAAeA,CAAA,EAAG;MAChB,IAAAC,qBAAA,GAA8B,IAAI,CAACP,eAAe;QAA1CC,OAAO,GAAAM,qBAAA,CAAPN,OAAO;QAAEC,QAAO,GAAAK,qBAAA,CAAPL,QAAO;MAExB,IAAI,CAACD,OAAM,IAAK,CAACC,QAAQ,EAAE;QACzBM,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;;MAEA;MACA,IAAMC,MAAK,GAAI,IAAI,CAACL,oBAAoB,CAACM,IAAI,CACzC,UAACC,CAAC;QAAA,OAAKA,CAAC,CAACV,OAAM,KAAMA,OAAM,IAAKU,CAAC,CAACT,QAAO,KAAMA,QAAO;MAAA,CAC1D,CAAC;MAED,IAAI,CAACO,MAAM,EAAE;QACXD,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;;MAEA;MACA,IAAI,IAAI,CAACL,aAAa,CAACS,IAAI,CAAC,UAACD,CAAC;QAAA,OAAKA,CAAC,CAACf,IAAG,KAAMa,MAAM,CAACb,IAAI;MAAA,EAAC,EAAE;QAC1DY,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;;MAEA;MACA,IAAI,CAACL,aAAa,CAACU,IAAI,CAAC;Q