{"ast":null,"code":"import \"core-js/modules/web.timers.js\";\nimport { Line } from 'vue-chartjs';\nimport { reactive, onMounted } from 'vue';\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale } from 'chart.js';\n\n// 注册 Chart.js 插件\nChartJS.register(Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale);\nexport default {\n  components: {\n    LineChart: Line\n  },\n  setup: function setup() {\n    // 定义 chartData，确保 datasets 初始为空\n    var chartData = reactive({\n      labels: [],\n      // 初始为空，数据加载后更新\n      datasets: [] // 初始为空，数据加载后更新\n    });\n\n    // 模拟异步数据加载\n    onMounted(function () {\n      setTimeout(function () {\n        chartData.labels = ['Jan', 'Feb', 'Mar', 'Apr', 'May'];\n        chartData.datasets = [{\n          label: '收入',\n          data: [1000, 1200, 1300, 1500, 1600],\n          borderColor: 'green',\n          fill: false\n        }, {\n          label: '支出',\n          data: [800, 900, 1000, 1100, 1200],\n          borderColor: 'red',\n          fill: false\n        }];\n      }, 1000); // 模拟1秒的异步加载时间\n    });\n    return {\n      chartData: chartData\n    };\n  }\n};","map":{"version":3,"names":["Line","reactive","onMounted","Chart","ChartJS","Title","Tooltip","Legend","LineElement","CategoryScale","LinearScale","register","components","LineChart","setup","chartData","labels","datasets","setTimeout","label","data","borderColor","fill"],"sources":["C:\\Users\\20915\\IdeaProjects\\financialmanagement\\src\\views\\AnalysisPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"analysis\">\r\n    <h2>财务数据分析</h2>\r\n    <line-chart :data=\"chartData\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Line } from 'vue-chartjs';\r\nimport { reactive, onMounted } from 'vue';\r\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale } from 'chart.js';\r\n\r\n// 注册 Chart.js 插件\r\nChartJS.register(Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale);\r\n\r\nexport default {\r\n  components: {\r\n    LineChart: Line,\r\n  },\r\n  setup() {\r\n    // 定义 chartData，确保 datasets 初始为空\r\n    const chartData = reactive({\r\n      labels: [],      // 初始为空，数据加载后更新\r\n      datasets: [],    // 初始为空，数据加载后更新\r\n    });\r\n\r\n    // 模拟异步数据加载\r\n    onMounted(() => {\r\n      setTimeout(() => {\r\n        chartData.labels = ['Jan', 'Feb', 'Mar', 'Apr', 'May'];\r\n        chartData.datasets = [\r\n          {\r\n            label: '收入',\r\n            data: [1000, 1200, 1300, 1500, 1600],\r\n            borderColor: 'green',\r\n            fill: false,\r\n          },\r\n          {\r\n            label: '支出',\r\n            data: [800, 900, 1000, 1100, 1200],\r\n            borderColor: 'red',\r\n            fill: false,\r\n          },\r\n        ];\r\n      }, 1000); // 模拟1秒的异步加载时间\r\n    });\r\n\r\n    return { chartData };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.analysis {\r\n  text-align: center;\r\n  margin-top: 50px;\r\n}\r\n</style>\r\n"],"mappings":";AAQA,SAASA,IAAG,QAAS,aAAa;AAClC,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,aAAa,EAAEC,WAAU,QAAS,UAAU;;AAE5G;AACAN,OAAO,CAACO,QAAQ,CAACN,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,aAAa,EAAEC,WAAW,CAAC;AAEjF,eAAe;EACbE,UAAU,EAAE;IACVC,SAAS,EAAEb;EACb,CAAC;EACDc,KAAK,WAALA,KAAKA,CAAA,EAAG;IACN;IACA,IAAMC,SAAQ,GAAId,QAAQ,CAAC;MACzBe,MAAM,EAAE,EAAE;MAAO;MACjBC,QAAQ,EAAE,EAAE,CAAK;IACnB,CAAC,CAAC;;IAEF;IACAf,SAAS,CAAC,YAAM;MACdgB,UAAU,CAAC,YAAM;QACfH,SAAS,CAACC,MAAK,GAAI,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;QACtDD,SAAS,CAACE,QAAO,GAAI,CACnB;UACEE,KAAK,EAAE,IAAI;UACXC,IAAI,EAAE,CAAC,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,CAAC;UACpCC,WAAW,EAAE,OAAO;UACpBC,IAAI,EAAE;QACR,CAAC,EACD;UACEH,KAAK,EAAE,IAAI;UACXC,IAAI,EAAE,CAAC,GAAG,EAAE,GAAG,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,CAAC;UAClCC,WAAW,EAAE,KAAK;UAClBC,IAAI,EAAE;QACR,CAAC,CACF;MACH,CAAC,EAAE,IAAI,CAAC,EAAE;IACZ,CAAC,