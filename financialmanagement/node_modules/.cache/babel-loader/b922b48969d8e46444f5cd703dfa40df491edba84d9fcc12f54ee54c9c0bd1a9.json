{"ast":null,"code":"import \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.reduce.js\";\nimport \"core-js/modules/es.object.keys.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.object.values.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport NavigationBar from \"../components/NavigationBar.vue\";\nimport Chart from \"chart.js/auto\";\nexport default {\n  name: \"IncomeStatsPage\",\n  components: {\n    NavigationBar: NavigationBar\n  },\n  props: {\n    transactions: {\n      type: Array,\n      required: true\n    }\n  },\n  computed: {\n    incomeTransactions: function incomeTransactions() {\n      return this.transactions.filter(function (transaction) {\n        return transaction.direction === \"收入\";\n      });\n    }\n  },\n  mounted: function mounted() {\n    this.renderIncomeChart();\n  },\n  methods: {\n    renderIncomeChart: function renderIncomeChart() {\n      // 获取收入数据，按商品名称分组统计金额\n      var incomeData = this.incomeTransactions.reduce(function (acc, curr) {\n        var key = curr.goods_name || \"其他\";\n        acc[key] = (acc[key] || 0) + curr.amount;\n        return acc;\n      }, {});\n      var ctx = document.getElementById(\"incomeChart\").getContext(\"2d\");\n      new Chart(ctx, {\n        type: \"pie\",\n        data: {\n          labels: Object.keys(incomeData),\n          datasets: [{\n            data: Object.values(incomeData),\n            backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\", \"#4BC0C0\", \"#9966FF\"]\n          }]\n        },\n        options: {\n          responsive: true,\n          maintainAspectRatio: false,\n          // 允许调整宽高\n          animation: {\n            duration: 1500,\n            // 动画持续时间\n            easing: \"easeOutBounce\" // 动画效果\n          },\n          plugins: {\n            legend: {\n              position: \"top\" // 图例位置\n            }\n          }\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavigationBar","Chart","name","components","props","transactions","type","Array","required","computed","incomeTransactions","filter","transaction","direction","mounted","renderIncomeChart","methods","incomeData","reduce","acc","curr","key","goods_name","amount","ctx","document","getElementById","getContext","data","labels","Object","keys","datasets","values","backgroundColor","options","responsive","maintainAspectRatio","animation","duration","easing","plugins","legend","position"],"sources":["C:\\Users\\20915\\IdeaProjects\\financialmanagement\\src\\views\\IncomeStatsPage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 导航栏 -->\r\n    <NavigationBar />\r\n\r\n    <!-- 收入统计表格 -->\r\n    <section class=\"content\">\r\n      <h2>收入统计</h2>\r\n      <table>\r\n        <thead>\r\n        <tr>\r\n          <th>ID</th>\r\n          <th>交易时间</th>\r\n          <th>交易类型</th>\r\n          <th>交易对象</th>\r\n          <th>商品名称</th>\r\n          <th>方向</th>\r\n          <th>金额</th>\r\n          <th>支付方式</th>\r\n          <th>交易状态</th>\r\n          <th>订单号</th>\r\n          <th>商户订单号</th>\r\n          <th>备注</th>\r\n        </tr>\r\n        </thead>\r\n        <tbody>\r\n        <tr v-for=\"(transaction, index) in incomeTransactions\" :key=\"index\">\r\n          <td>{{ transaction.id }}</td>\r\n          <td>{{ transaction.trans_time }}</td>\r\n          <td>{{ transaction.trans_type }}</td>\r\n          <td>{{ transaction.trans_party }}</td>\r\n          <td>{{ transaction.goods_name }}</td>\r\n          <td>{{ transaction.direction }}</td>\r\n          <td>{{ transaction.amount }}</td>\r\n          <td>{{ transaction.payment_method }}</td>\r\n          <td>{{ transaction.trans_status }}</td>\r\n          <td>{{ transaction.order_no }}</td>\r\n          <td>{{ transaction.merchant_order_no }}</td>\r\n          <td>{{ transaction.note }}</td>\r\n        </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- 饼状图 -->\r\n      <h3>收入分类统计</h3>\r\n      <div class=\"chart-container\">\r\n        <canvas id=\"incomeChart\"></canvas>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavigationBar from \"../components/NavigationBar.vue\";\r\nimport Chart from \"chart.js/auto\";\r\n\r\nexport default {\r\n  name: \"IncomeStatsPage\",\r\n  components: {\r\n    NavigationBar,\r\n  },\r\n  props: {\r\n    transactions: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  computed: {\r\n    incomeTransactions() {\r\n      return this.transactions.filter((transaction) => transaction.direction === \"收入\");\r\n    },\r\n  },\r\n  mounted() {\r\n    this.renderIncomeChart();\r\n  },\r\n  methods: {\r\n    renderIncomeChart() {\r\n      // 获取收入数据，按商品名称分组统计金额\r\n      const incomeData = this.incomeTransactions.reduce((acc, curr) => {\r\n        const key = curr.goods_name || \"其他\";\r\n        acc[key] = (acc[key] || 0) + curr.amount;\r\n        return acc;\r\n      }, {});\r\n\r\n      const ctx = document.getElementById(\"incomeChart\").getContext(\"2d\");\r\n\r\n      new Chart(ctx, {\r\n        type: \"pie\",\r\n        data: {\r\n          labels: Object.keys(incomeData),\r\n          datasets: [\r\n            {\r\n              data: Object.values(incomeData),\r\n              backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\", \"#4BC0C0\", \"#9966FF\"],\r\n            },\r\n          ],\r\n        },\r\n        options: {\r\n          responsive: true,\r\n          maintainAspectRatio: false, // 允许调整宽高\r\n          animation: {\r\n            duration: 1500, // 动画持续时间\r\n            easing: \"easeOutBounce\", // 动画效果\r\n          },\r\n          plugins: {\r\n            legend: {\r\n              position: \"top\", // 图例位置\r\n            },\r\n          },\r\n        },\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.content 