ton @click=\"removeFamilyMember(index)\" class=\"remove-btn\">删除</button>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n\r\n        <!-- 添加家庭成员 -->\r\n        <div class=\"add-member\">\r\n          <input v-model=\"newFamilyMember\" type=\"text\" placeholder=\"输入家庭成员账号\" />\r\n          <button @click=\"addFamilyMember\" class=\"add-btn\">添加家庭成员</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavigationBar from '@/components/NavigationBar.vue';\r\n\r\nexport default {\r\n  name: \"MyPage\",\r\n  components: {\r\n    NavigationBar,\r\n  },\r\n  data() {\r\n    return {\r\n      userId: \"12345\", // 示例用户ID\r\n      newFamilyMember: \"\", // 新添加家庭成员的账号\r\n      familyMembers: [], // 存储家庭成员账号的数组\r\n    };\r\n  },\r\n  methods: {\r\n    // 添加家庭成员\r\n    addFamilyMember() {\r\n      if (this.newFamilyMember.trim()) {\r\n        if (this.familyMembers.includes(this.newFamilyMember)) {\r\n          alert(\"该家庭成员已存在\");\r\n        } else {\r\n          this.familyMembers.push(this.newFamilyMember);\r\n          this.newFamilyMember = \"\"; // 清空输入框\r\n        }\r\n      } else {\r\n        alert(\"请输入有效的账号名\");\r\n      }\r\n    },\r\n\r\n    // 删除家庭成员\r\n    removeFamilyMember(index) {\r\n      this.familyMembers.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-page-content {\r\n  margin: 20px;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.user-info {\r\n  margin-top: 20px;\r\n}\r\n\r\n.family-members {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.family-members ul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.family-members li {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  margin-bottom: 5px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.remove-btn {\r\n  background-color: #dc3545;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-btn:hover {\r\n  background-color: #c82333;\r\n}\r\n\r\n.add-member {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.add-member input {\r\n  padding: 8px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  width: 250px;\r\n}\r\n\r\n.add-btn {\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  padding: 8px 15px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.add-btn:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AAkCA,OAAOA,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVF,aAAa,EAAbA;EACF,CAAC;EACDG,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,OAAO;MAAE;MACjBC,eAAe,EAAE,EAAE;MAAE;MACrBC,aAAa,EAAE,EAAE,CAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,eAAe,WAAfA,eAAeA,CAAA,EAAG;MAChB,IAAI,IAAI,CAACH,eAAe,CAACI,IAAI,CAAC,CAAC,EAAE;QAC/B,IAAI,IAAI,CAACH,aAAa,CAACI,QAAQ,CAAC,IAAI,CAACL,eAAe,CAAC,EAAE;UACrDM,KAAK,CAAC,UAAU,CAAC;QACnB,OAAO;UACL,IAAI,CAACL,aAAa,CAACM,IAAI,CAAC,IAAI,CAACP,eAAe,CAAC;UAC7C,IAAI,CAACA,eAAc,GAAI,EAAE,EAAE;QAC7B;MACF,OAAO;QACLM,KAAK,CAAC,WAAW,CAAC;MACpB;IACF,CAAC;IAED;IACAE,kBAAkB,WAAlBA,kBAAkBA,CAACC,KAAK,EAAE;MACxB,IAAI,CAACR,aAAa,CAACS,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IACrC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}